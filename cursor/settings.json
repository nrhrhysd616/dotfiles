  "editor.wordWrapColumn": 100,
  "editor.wrappingIndent": "same",
  "editor.wrappingStrategy": "simple",
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "off",
  "editor.cursorStyle": "line",
  "editor.cursorSurroundingLines": 20,
  "editor.cursorSurroundingLinesStyle": "default",
  "editor.cursorWidth": 2,
  "editor.find.addExtraSpaceOnTop": true,
  "editor.find.autoFindInSelection": "never",
  "editor.find.cursorMoveOnType": true,
  "editor.find.globalFindClipboard": true,
  "editor.find.loop": true,
  "editor.find.seedSearchStringFromSelection": "always",
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  // フォントの合字を有効にするか(詳細はhttps://github.com/tonsky/FiraCode などで)
  "editor.fontLigatures": true,
  "editor.fontSize": 11.5,
  "editor.fontVariations": false,
  "editor.fontWeight": "normal",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnType": true,
  // ここまで完了

  // ## 差分エディタ設定
  "diffEditor.codeLens": true,
  // 差分エディタの差分を表示するアルゴリズム
  "diffEditor.diffAlgorithm": "advanced",
  // 差分エディタのときに空白をハイライトするか
  "diffEditor.experimental.showEmptyDecorations": true,
  // 差分エディタのときに移動を検知するか
  "diffEditor.experimental.showMoves": true,
  // 差分エディタのときに変更されていない領域でのコンテキストの表示行数
  "diffEditor.hideUnchangedRegions.contextLineCount": 3,
  // 差分エディタのときに変更されていない領域を表示にするか
  "diffEditor.hideUnchangedRegions.enabled": false,
  "diffEditor.hideUnchangedRegions.minimumLineCount": 10,
  "diffEditor.hideUnchangedRegions.revealLineCount": 20,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "files.autoSave": "onFocusChange",
  "workbench.colorTheme": "Monokai Pro",
  "workbench.editor.decorations.colors": true,
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.sideBar.location": "left",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.enablePreview": false,
  "explorer.autoReveal": false,
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "line",
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.enableSmartCommit": true,
  "git.path": "/usr/local/bin/git",
  "git-graph.date.format": "ISO Date & Time",
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "${userHome}/.sdkman/candidates/java/11.0.17-amzn"
    },
    {
      "name": "JavaSE-17",
      "path": "${userHome}/.sdkman/candidates/java/17.0.5-amzn"
    },
    {
      "name": "JavaSE-18",
      "path": "${userHome}/.sdkman/candidates/java/18.0.2-amzn",
      "default": true
    }
  ],
  "java.format.settings.url": "https://raw.githubusercontent.com/nrhrhysd616/dotfiles/main/java/java-formatter-profile-nrhrhysd616-google-style.xml",
  "java.import.generatesMetadataFilesAtProjectRoot": true,
  "java.import.gradle.java.home": "${userHome}/.sdkman/candidates/java/17.0.5-amzn",
  "java.jdt.ls.java.home": "${userHome}/.sdkman/candidates/java/18.0.2-amzn",
  "java.referencesCodeLens.enabled": true,
  "todo-tree.general.tags": ["BUG", "FIXME", "TODO", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "protoc": {
    "path": "/usr/local/bin/protoc"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "files.trimFinalNewlines": true
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "diffEditor.ignoreTrimWhitespace": false
  },
  "github.copilot.editor.enableCodeActions": false,
  "prettier.printWidth": 100,
  "editor.minimap.enabled": true,
  "workbench.activityBar.location": "top",
  "git.autofetch": true,
  "github.copilot.editor.enableAutoCompletions": true,
  // 言語別設定
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  // 拡張機能設定は以下に記述
  "redhat.telemetry.enabled": false,
  // bmewburn.vscode-intelephense-client
  "intelephense.format.braces": "k&r",
  // Prisma.prisma
  "prisma.showPrismaDataPlatformNotification": false
}
