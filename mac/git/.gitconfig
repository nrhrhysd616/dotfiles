[gpg]
	program = gpg
[include]
	path = ~/.gitconfig.user.local
[core]
	excludesfile = ~/.gitignore_global
	pager = LESSCHARSET=utf-8 less
	quotepath = false
	ignorecase = false
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	rebase = false
[commit]
        gpgsign = true
[tag]
	gpgsign = true
